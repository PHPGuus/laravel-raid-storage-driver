{
    "name": "phpguus/laravel-raid-storage-driver",
    "description": "A Laravel Storage driver that encapsulates the Flysystem RAID driver.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "PHPGuus",
            "email": "php.guus@gmail.com"
        }
    ],
	"require": {
		"php": "^7.2",
        "phpguus/flysystem-raid": "^1.0",
        "illuminate/support": "5.8.*",
        "illuminate/console": "5.8.*"
    },
	"require-dev": {
        "phpunit/phpunit": "^8.3"
    },
	"autoload": {
		"psr-4": {
			"PHPGuus\\RaidStorage\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PHPGuus\\RaidStorage\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"PHPGuus\\RaidStorage\\ServiceProvider"
			]
		}
	}
}
